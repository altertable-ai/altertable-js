{
  "name": "@altertable/altertable-snippet",
  "description": "JavaScript snippet for loading and initializing Altertable analytics tracking",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "homepage": "https://github.com/altertable-ai/altertable-js",
  "repository": "altertable-ai/altertable-js",
  "author": {
    "name": "Altertable",
    "url": "https://altertable.ai"
  },
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint index.js",
    "build:es5": "babel index.js --out-file dist/index.js",
    "build:min": "terser index.js -c -m -o dist/index.min.js",
    "build": "bun run build:es5 && bun run build:min",
    "dev": "bun run build:es5 --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "terser": "^5.38.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "ie": "9"
          }
        }
      ]
    ]
  }
}
